<?xml version="1.0"?>
<config>
    <modules>
        <SP_CustomShipping>
            <version>0.1.0</version>
        </SP_CustomShipping>
    </modules>
    <global>
        <helpers>
            <sp_customshipping>
                <class>SP_CustomShipping_Helper</class>
            </sp_customshipping>
        </helpers>
        <models>
            <sp_customshipping>
                <class>SP_CustomShipping_Model</class>
            </sp_customshipping>
        </models>
    </global>
    <default>
        <carriers>
            <sp_customshipping>
                <active>1</active>
                <model>sp_customshipping/carrier</model>
                <title>Spalah Shipping Carrier</title>
                <sort_order>10</sort_order>
                <sallowspecific>0</sallowspecific>
                <express_max_weight>1</express_max_weight>
            </sp_customshipping>
        </carriers>
    </default>
</config>